<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FormaGym" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#3E3E3F</item>
        <!-- Customize your theme here. -->
        <item name="android:windowIsTranslucent">true</item>
        <item name="background">@android:color/transparent</item>
        <item name="android:navigationBarColor">@color/grey</item>
    </style>

    <style name="et_style">
        <item name="hintTextColor">@color/grey</item>
        <item name="strokeColor">@color/grey</item>
    </style>
</resources>